CREATE TABLE dwh.silver.country_state_dim (
	country_state_sk BIGINT GENERATED ALWAYS AS IDENTITY,
	country STRING,
	state STRING
)

CREATE TABLE dwh.silver.customer_dim (
	customer_sk INT,
	gender STRING,
	name STRING,
	country_state_sk BIGINT
)

CREATE TABLE dwh.silver.store_dim (
	store_sk INT,
	country_state_sk BIGINT,
	square_meters INT,
	open_date DATE
)

CREATE TABLE dwh.silver.category_dim (
	category_sk BIGINT GENERATED ALWAYS AS IDENTITY,
	category STRING,
	subcategory STRING
)

CREATE TABLE dwh.silver.product_dim (
	product_sk INT,
	product_name STRING,
	brand STRING,
	color STRING,
	unit_cost_usd DECIMAL(10,2),
	unit_price_usd DECIMAL(10,2),
	category_sk BIGINT
)

CREATE TABLE dwh.silver.currency_dim (
	currency_sk BIGINT GENERATED ALWAYS AS IDENTITY,
	date DATE,
	currency STRING,
	exchange DECIMAL(8,5)
)







	category_sk BIGINT
)